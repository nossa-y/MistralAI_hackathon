			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Date of prorogation applied -->
	<xs:element name="fr-date-prorogation-applied">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
