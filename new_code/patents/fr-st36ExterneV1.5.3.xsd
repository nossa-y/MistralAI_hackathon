	<!--[V1.5.3] ref note claims modified - See fr-amended-claim" <xs:element name="fr-date-claim-modified"> <xs:complexType> <xs:sequence> 
		<xs:element ref="date"/> <xs:element ref="fr-bopinum" minOccurs="0"/> </xs:sequence> </xs:complexType> </xs:element> -->
	<!-- Date of search supplemental -->
	<xs:element name="fr-date-search-supplemental">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
