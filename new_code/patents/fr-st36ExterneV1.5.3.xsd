	. fr-date-lapsed : replace field document-id by only date field . fr-last-fee-payment : element text is now available . Add an attribute 
	Sequence for fr-publication-number (for CCP) 2008-10-07 . fr-date-availability => deleted from fr-references-cited (move to fr-patent-life) 
	. fr-references-cited becomes references-cited . add element text for fr-inscription v1.02.00 2009-02-25 . fr-date-availability => add 
	following elements : - <fr-date-prorogation-published> - <fr-date-prorogation-applied> - <fr-date-limit-prorogation> - <fr-date-prorogation-refused> 
	v1.4.00 2009-10-09 . Add following elements in fr-bibliographic-data : - <fr-owners> / <fr-owner 1,unbounded> <addressbook> - <fr-licencees> 
	/ <licencee 1,unbounded> <addressbook> Sup corresponding elements from fr-inscription: <assignees, licencees> v1.5.0 fr-ccp-data adjustement 
	v1.5.1 2012-05-11 Adjustement for XML generation with jaxb <fr-applicant reference> added <fr-date-claim-modified> added v1.5.2 2012-08-xx 
	Ajustement for ST36+ (Project inpi-diff-brevet optimization) . Search for "[V1.5.2]" comment v1.5.3 2013-10-xx Ajustement for ST36+ (Project 
	inpi-diff-brevet) . Search for "[V1.5.2]" comment ***** End Revision History ***** -->
<!-- Entities definition -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<xs:complexType name="address_group">
		<xs:choice>
			<xs:sequence>
				<xs:element ref="address-1" minOccurs="0"/>
				<xs:element ref="address-2" minOccurs="0"/>
				<xs:element ref="address-3" minOccurs="0"/>
				<xs:element ref="mailcode" minOccurs="0"/>
				<xs:element ref="pobox" minOccurs="0"/>
				<xs:element ref="room" minOccurs="0"/>
				<xs:element ref="address-floor" minOccurs="0"/>
				<xs:element ref="building" minOccurs="0"/>
				<xs:element ref="street" minOccurs="0"/>
				<xs:element ref="city" minOccurs="0"/>
				<xs:element ref="county" minOccurs="0"/>
				<xs:element ref="state" minOccurs="0"/>
				<xs:element ref="postcode" minOccurs="0"/>
				<xs:element ref="country"/>
			</xs:sequence>
			<xs:element ref="text"/>
		</xs:choice>
	</xs:complexType>
	<xs:group name="name_group">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="name"/>
				<xs:sequence>
					<xs:element ref="prefix" minOccurs="0"/>
					<xs:choice>
						<xs:element ref="last-name"/>
						<xs:element ref="orgname"/>
					</xs:choice>
					<xs:element ref="first-name" minOccurs="0"/>
					<xs:element ref="middle-name" minOccurs="0"/>
					<xs:element ref="suffix" minOccurs="0"/>
					<xs:element ref="iid" minOccurs="0"/>
					<xs:element ref="role" minOccurs="0"/>
					<xs:element ref="department" minOccurs="0"/>
					<xs:element ref="synonym" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
			<xs:element ref="registered-number" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<!-- ********************************************* -->
	<!-- level 0 : root element of the DTD. -->
	<!-- ********************************************* -->
	<!-- level 0 : root element of the DTD. -->
	<xs:element name="fr-patent-document">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-bibliographic-data"/>
				<xs:element ref="abstract" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fr-patent-life" minOccurs="0"/>
				<xs:element ref="fr-ccp-data" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-patent-document_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-patent-document_attlist">
		<xs:attribute name="lang" use="required"/>
		<xs:attribute name="dtd-version"/>
		<xs:attribute name="file"/>
		<xs:attribute name="status"/>
		<xs:attribute name="id" use="required"/>
		<xs:attribute name="country" use="required"/>
		<xs:attribute name="file-reference-id"/>
		<xs:attribute name="correction-code"/>
		<xs:attribute name="doc-number"/>
		<xs:attribute name="kind"/>
		<xs:attribute name="date-produced"/>
		<xs:attribute name="family-id"/>
	</xs:attributeGroup>
	<!-- level 1: Bibliographic-data definition -->
	<xs:element name="fr-bibliographic-data">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-publication-data" minOccurs="0"/>
				<!-- [V1.5.3] -->
				<xs:element ref="fr-application-reference" minOccurs="0" maxOccurs="unbounded"/>
				<!-- [/V1.5.3] -->
				<xs:element ref="related-documents" minOccurs="0"/>
				<xs:element ref="invention-title" minOccurs="0"/>
				<xs:element ref="fr-keywords" minOccurs="0"/>
				<xs:element ref="parties" minOccurs="0"/>
				<xs:element ref="fr-owners" minOccurs="0"/>
				<!-- [V1.5.1] ref fr-applicant-reference -->
				<xs:element ref="fr-applicant-reference" minOccurs="0"/>
				<!-- [/V1.5.1] -->
				<xs:element ref="fr-licensees" minOccurs="0"/>
				<xs:element ref="classification-ipc" minOccurs="0"/>
				<xs:element ref="classifications-ipcr" minOccurs="0"/>
				<!-- [V1.5.3] -->
				<xs:element ref="patent-classifications" minOccurs="0"/>
				<xs:element ref="language-of-filing" minOccurs="0"/>
				<!-- [/V1.5.3] -->
				<xs:element ref="fr-priority-claims" minOccurs="0"/>
				<xs:element ref="fr-extension" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-bibliographic-data_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-bibliographic-data_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="lang"/>
		<xs:attribute name="status"/>
		<xs:attribute name="country"/>
	</xs:attributeGroup>
	<!-- level 2 : bibliographic-data => publication data : reference and others informations -->
	<xs:element name="fr-publication-data">
		<xs:complexType>
			<xs:sequence>
				<!-- [V1.5.3] maxOccurs -->
				<xs:element ref="fr-publication-reference" minOccurs="0" maxOccurs="unbounded"/>
				<!-- /[V1.5.3] -->
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : Publication content -->
	<!-- publication reference information: publication number, date, nature ... -->
	<xs:element name="fr-publication-reference">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
				<xs:element ref="fr-nature" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-publication-reference_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-publication-reference_attlist">
		<xs:attribute name="sequence"/>
		<!-- [V1.5.3] -->
		<xs:attribute name="doc-id" type="xs:string" use="optional"/>
		<xs:attribute name="data-format" type="xs:string" use="optional"/>
		<!-- /[V1.5.3] -->
	</xs:attributeGroup>
	<!-- Sequence of the CCP -->
	<!-- level 2 : bibliographic-data => application reference information: application number, country -->
	<xs:element name="fr-application-reference">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-application-reference_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-application-reference_attlist">
		<xs:attribute name="appl-type" use="optional"/>
		<!-- [1.5.3] -->
		<xs:attribute name="doc-id" type="xs:string" use="optional"/>
		<xs:attribute name="data-format" type="xs:string" use="optional"/>
		<!-- /[1.5.3] -->
	</xs:attributeGroup>
	<!-- level 2 : bibliographic-data => publication data : reference and others informations -->
	<xs:element name="related-documents">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="addition"/>
				<xs:element ref="division"/>
				<xs:element ref="continuation"/>
				<xs:element ref="continuation-in-part"/>
				<xs:element ref="continuing-reissue"/>
				<xs:element ref="reissue"/>
				<xs:element ref="reexamination"/>
				<xs:element ref="substitution"/>
				<xs:element ref="utility-model-basis"/>
				<xs:element ref="correction"/>
				<xs:element ref="related-publication"/>
				<xs:element ref="related-application"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : content of related-documents -->
	<!-- Earlier document to which this Is an addition. INID 61, ST.32 equivalency = B610 -->
	<xs:element name="addition">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="parent-doc" minOccurs="0"/>
				<xs:element ref="child-doc" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="division">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="parent-doc" minOccurs="0"/>
				<xs:element ref="child-doc" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- Document being reissued. INID 64. ST.32 equivalency = B640 -->
	<xs:element name="continuation" type="relation"/>
	<xs:element name="continuation-in-part" type="relation"/>
	<xs:element name="continuing-reissue" type="relation"/>
	<xs:element name="reissue" type="relation"/>
	<!-- Document of which this is a reexamination. ST.32 equivalency = B645 -->
	<xs:element name="reexamination" type="relation"/>
	<!-- Document for which this is a substitute. INID 66. ST.32 equivalency = B660 -->
	<xs:element name="substitution" type="relation"/>
	<!-- Document on which utility model is based. INID 67. ST.32 equivalency = B670 -->
	<xs:element name="utility-model-basis">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="parent-doc" minOccurs="0"/>
				<xs:element ref="child-doc" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="correction">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-corrected"/>
				<xs:element ref="type-of-correction"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- References to other legally or procedurally related domestic patent documents. INID 60; ST.32 equivalency = B600 -->
	<!-- Previously published document concerning same application. INID 65. ST.32 equivalency = B650 -->
	<xs:element name="related-publication">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="document-id" minOccurs="0" maxOccurs="unbounded"/>
				<!-- V[1.5.3] -->
				<xs:element ref="fr-bopinum" minOccurs="0"/>
				<!-- /V[1.5.3] -->
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- [V1.5.3] Related application concerning this CCP -->
	<xs:element name="related-application">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="document-id" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- [V1.5.3] -->
	<!-- level 4 -->
	<xs:complexType name="relation">
		<xs:sequence>
			<xs:element ref="parent-doc" minOccurs="0"/>
			<xs:element ref="child-doc" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<!-- level 5 : relation content -->
	<!-- Describes parent document. ST.32 equivalency = PARENT -->
	<xs:element name="parent-doc">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
				<xs:element ref="parent-status" minOccurs="0"/>
				<xs:element ref="parent-grant-document" minOccurs="0"/>
				<xs:element ref="parent-pct-document" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 6 : parent-doc content -->
	<xs:element name="parent-grant-document">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="parent-pct-document">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="parent-status" type="xs:string"/>
	<!-- level 5 : Identifies child document. -->
	<xs:element name="child-doc">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 4 -->
	<xs:element name="document-corrected">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Type of correction. ST.32 equivalency = B667 -->
	<xs:element name="type-of-correction" type="xs:string"/>
	<!-- level 2 : bibliographic-data =>invention title -->
	<!-- Title of the Invention: preferably two to seven words when in English or translated into English and precise. (Rule 4.3) Recommended 
		id = 'invt' -->
	<xs:element name="invention-title">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
			</xs:choice>
			<xs:attributeGroup ref="invention-title_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="invention-title_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="lang"/>
	</xs:attributeGroup>
	<!-- level 2 : bibliographic-data => keywords -->
	<xs:element name="fr-keywords">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="keyword" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 2 : bibliographic-data => applicants, inventors, agents -->
	<!-- PCT/RO/101 Box No.II, III and IV APPLICANTS, FURTHER APPLICANT(S) AND/OR (FURTHER) INVENTOR(S)AGENT OR COMMON REPRESENTATIVE; OR 
		ADDRESS FOR CORRESPONDENCE (PCT Rule 4.5, 4.6, 4.7, 4.8) -->
	<xs:element name="parties">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="applicants"/>
				<xs:element ref="inventors" minOccurs="0"/>
				<xs:element ref="agents" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : Applicants information: At least one of the applicants named must be a resident or national of a PCT Contracting State 
		for which the receiving Office acts. -->
	<xs:element name="applicants">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="applicant" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 4 : Applicant information: attributes for applicant sequence : sequence number of each applicant e.g, sequence="1" app-type 
		: applicant type app-type="applicant" : the applicant is only applicant app-type="applicant-inventor" : the applicant is also inventor 
		designation: This person is applicant for the purpose of: designation="all" : all designated states designation="all-except-us" : all 
		designated states except US designation="us-only" : US only designation="as-indicated" : the states indicated in designated-states element 
		under applicant element -->
	<xs:element name="applicant">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook" maxOccurs="unbounded"/>
				<xs:element ref="nationality" minOccurs="0"/>
				<xs:element ref="residence" minOccurs="0"/>
				<xs:element ref="us-rights" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="designated-states" minOccurs="0"/>
				<xs:element ref="designated-states-as-inventor" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="applicant_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="applicant_attlist">
		<xs:attribute name="sequence" use="required"/>
		<xs:attribute name="app-type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="applicant"/>
					<xs:enumeration value="applicant-inventor"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="designation" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="all"/>
					<xs:enumeration value="all-except-us"/>
					<xs:enumeration value="us-only"/>
					<xs:enumeration value="as-indicated"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- level 3 : Inventors information: If the US is designated, all of the inventors must also be named as applicants for purposes of 
		that designation. -->
	<xs:element name="inventors">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="inventor"/>
				<xs:element ref="deceased-inventor"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- level 4 : the individual or entity responsible for creating the matter that protection is sought for -->
	<xs:element name="inventor">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook" maxOccurs="unbounded"/>
				<xs:element ref="designated-states" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="inventor_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="inventor_attlist">
		<xs:attribute name="sequence" use="required"/>
		<xs:attribute name="designation">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="all"/>
					<xs:enumeration value="all-except-us"/>
					<xs:enumeration value="us-only"/>
					<xs:enumeration value="as-indicated"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- level 3 : Agents information : information regarding Agents or common representatives -->
	<xs:element name="agents">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="customer-number"/>
				<xs:element ref="agent" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- level 4 : content of Agents -->
	<!-- Customer number for agents by office -->
	<xs:element name="customer-number" type="xs:string"/>
	<!-- Agent or common representative (Rules 90.1, 90.2) -->
	<xs:element name="agent">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="agent_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="agent_attlist">
		<xs:attribute name="sequence" use="required"/>
		<xs:attribute name="rep-type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="agent"/>
					<xs:enumeration value="attorney"/>
					<xs:enumeration value="common-representative"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- level 2 : Grantees/owners information. -->
	<xs:element name="fr-owners">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-owner" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : owner name and address. -->
	<xs:element name="fr-owner">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook"/>
			</xs:sequence>
			<xs:attribute name="sequence"/>
		</xs:complexType>
	</xs:element>

	<!--[V1.5.1] def fr-applicant-reference - Application reference used for Gazette publication -->
	<xs:element name="fr-applicant-reference" type="xs:string"/>
	<!--[/V1.5.1] -->
	<!-- level 2 : Licensee Data. ST.32 equivalency = B790 -->
	<xs:element name="fr-licensees">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="licensee" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : Licensee name and address. ST.32 equivalency = B791 -->
	<xs:element name="licensee">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook"/>
			</xs:sequence>
			<xs:attribute name="sequence"/>
			<xs:attributeGroup ref="licensee_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="licensee_attlist">
		<xs:attribute name="licensee-type" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Normale"/>
					<xs:enumeration value="Exclusive"/>
					<xs:enumeration value="Unique"/>
					<xs:enumeration value="Obligatoire"/>
					<xs:enumeration value="N/A"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- level 4 : Licensee-registered -->
	<xs:element name="fr-licensee-registered">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="registered-number"/>
				<xs:element ref="date"/>
				<xs:element ref="fr-nature-inscription"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 2 : bibliographic-data => classifications -->
	<!-- International Patent Classification (IPC) data. INID 51. ST.32 equivalency = B510 -->
	<xs:element name="classification-ipc">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="edition"/>
				<xs:element ref="main-classification"/>
				<xs:element ref="further-classification" minOccurs="0" maxOccurs="unbounded"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="additional-info"/>
					<xs:element ref="linked-indexing-code-group"/>
					<xs:element ref="unlinked-indexing-code"/>
				</xs:choice>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="classification-ipc_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="classification-ipc_attlist">
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- level 3 : content of classification-ipc -->
	<!-- main classification. ST.32 equivalency = B511/B521 -->
	<xs:element name="main-classification" type="xs:string"/>
	<!-- further/secondary classification ST.32 equivalency = B512/B522 -->
	<xs:element name="further-classification">
		<xs:complexType mixed="true">
			<xs:attributeGroup ref="further-classification_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="further-classification_attlist">
		<xs:attribute name="sequence"/>
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- Additional information. ST.32 equivalency = B513 -->
	<xs:element name="additional-info" type="xs:string"/>
	<!-- Linked indexing code. ST.32 equivalency = B514/B524 -->
	<xs:element name="linked-indexing-code-group">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="main-linked-indexing-code"/>
				<xs:element ref="sub-linked-indexing-code" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Main Linked Indexing Code. -->
	<xs:element name="main-linked-indexing-code" type="xs:string"/>
	<!-- sub/further Linked Indexing Code. -->
	<xs:element name="sub-linked-indexing-code" type="xs:string"/>
	<!-- Unlinked indexing code. ST.32 equivalency = B515/B525 -->
	<xs:element name="unlinked-indexing-code" type="xs:string"/>
	<!-- level 2 : Use sequence attribute to indicate the order in which the IPCR should be displayed. -->
	<!-- level 2 : Use sequence attribute to indicate the order in which the IPCR should be displayed. -->
	<xs:element name="classifications-ipcr">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-ipcr" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-ipcr">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="ipc-version-indicator"/>
					<xs:element ref="classification-level" minOccurs="0"/>
					<xs:element ref="section"/>
					<xs:element ref="class"/>
					<xs:element ref="subclass"/>
					<xs:element ref="main-group" minOccurs="0"/>
					<xs:element ref="subgroup" minOccurs="0"/>
					<xs:element ref="symbol-position" minOccurs="0"/>
					<xs:element ref="classification-value" minOccurs="0"/>
					<xs:element ref="action-date" minOccurs="0"/>
					<xs:element ref="generating-office" minOccurs="0"/>
					<xs:element ref="classification-status" minOccurs="0"/>
					<xs:element ref="classification-data-source" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="text"/>
			</xs:choice>
			<xs:attributeGroup ref="classification-ipcr_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="classification-ipcr_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="sequence"/>
	</xs:attributeGroup>
	<!-- level 3 : content of classification-ipcr -->
	<!-- IPCR version is a date. -->
	<xs:element name="ipc-version-indicator">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- IPCR levels: advanced-level, core-level, subclass-level -->
	<xs:element name="classification-level" type="xs:string"/>
	<!-- IPC section -->
	<xs:element name="section" type="xs:string"/>
	<!-- IPC subclass -->
	<xs:element name="subclass" type="xs:string"/>
	<!-- IPC main group -->
	<xs:element name="main-group" type="xs:string"/>
	<!-- IPC subgroup -->
	<xs:element name="subgroup" type="xs:string"/>
	<!-- Possible values: first-symbol-position, later-symbol-position. -->
	<xs:element name="symbol-position" type="xs:string"/>
	<!-- Possible values: invention, non-invention -->
	<xs:element name="classification-value" type="xs:string"/>
	<!-- The date of assigning the classification symbol (action date). -->
	<xs:element name="action-date">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Since part of the original data in the advanced level and the reclassified data can be delivered by offices other than the publishing 
		office, the information source of such data is recorded. -->
	<xs:element name="generating-office">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Possible status values: original-classification, reclassified, various-classifications, deleted-classification. This represents 
		the status of the classifications assigned. -->
	<xs:element name="classification-status" type="xs:string"/>
	<!-- Possible source values: human-generated-classification, machine-propagated-classification, software-analysis-generated-classification. 
		Machine propagated means that a classification assigned to one family member is automatically assigned to other members of the family. 
		Software-analysis generated means that the choice of classification was made by automated analysis of the document. -->
	<xs:element name="classification-data-source" type="xs:string"/>
	<!-- level 2 : bibliographic-data => PCT/RO/101 BOX No.VI Priority Claim (PCT Rule 4.10) -->
	<xs:element name="fr-priority-claims">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-priority-claim" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : priority claim (PCT Rule 4.10) -->
	<xs:element name="fr-priority-claim">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element ref="doc-number" minOccurs="0"/>
				<xs:element ref="date"/>
				<xs:element ref="office-of-filing" minOccurs="0"/>
				<xs:choice minOccurs="0">
					<xs:element ref="priority-doc-requested"/>
					<xs:element ref="priority-doc-attached"/>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="fr-priority-claim_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-priority-claim_attlist">
		<xs:attribute name="sequence" use="required"/>
		<xs:attribute name="kind">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="national"/>
					<xs:enumeration value="regional"/>
					<xs:enumeration value="international"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="id" type="xs:ID"/>
		<!-- [V1.5.3] -->
		<xs:attribute name="data-format" type="xs:string" use="optional"/>
		<!-- /[V1.5.3] -->
	</xs:attributeGroup>
	<!-- level 4 : Content of priority-claim -->
	<!-- Where the earlier application is an ARIPO application,indicate at least one country party to the PARIS convention for the protection 
		of industrial property or one member of the WTO for which that earlier application was filed. -->
	<xs:element name="office-of-filing">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="region"/>
					<xs:element ref="country" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="country"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- the receiving office is requested to prepare and transmit to the IB a certified copy of the earlier application -->
	<xs:element name="priority-doc-requested">
		<xs:complexType/>
	</xs:element>
	<!-- the priority document is attached to the application -->
	<xs:element name="priority-doc-attached">
		<xs:complexType/>
	</xs:element>
	<!-- End of bibliographic-data -->
	<!-- level 1 : Patent abstract -->
	<!-- ABstrACT: PCT Rule 8 A concise summary of the disclosure, 50-150 words. Recommended id = 'abst' -->
	<xs:element name="abstract">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="doc-page" maxOccurs="unbounded"/>
				<xs:sequence>
					<xs:element ref="abst-problem"/>
					<xs:element ref="abst-solution"/>
				</xs:sequence>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attributeGroup ref="abstract_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="abstract_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="lang"/>
		<xs:attribute name="status"/>
		<xs:attribute name="data-format"/>
	</xs:attributeGroup>
	<!-- level 2 : abstract => content of abstract -->
	<!-- the alleged problem that the invention purports to solve Recommended id = 'absp' -->
	<xs:element name="abst-problem">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="abst-problem_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="abst-problem_attlist">
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- the solution to the alleged problem that the invention purports to solve. Recommended id = 'abss' -->
	<xs:element name="abst-solution">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="abst-solution_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="abst-solution_attlist">
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- doc-page - DOCument or PAGE This is primarily to be used when data for the whole or major sections of an application are in image 
		format OR other formats such as pdf. Page range attributes are given when required pp- page range ppf - first page ppl - last page (for 
		pdf). The 'img' may also be used for complete pages, eg. in drawings Recommended id = docp01, docp02, etc. -->
	<xs:element name="doc-page">
		<xs:complexType>
			<xs:attributeGroup ref="doc-page_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="doc-page_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="he" use="required"/>
		<xs:attribute name="wi" use="required"/>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="jpg"/>
					<xs:enumeration value="pdf"/>
					<xs:enumeration value="tif"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="alt"/>
		<xs:attribute name="pp"/>
		<xs:attribute name="ppf"/>
		<xs:attribute name="ppl"/>
		<xs:attribute name="file" use="required"/>
		<xs:attribute name="color">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="color"/>
					<xs:enumeration value="bw"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="orientation" default="portrait">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="portrait"/>
					<xs:enumeration value="landscape"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- End of patent abstract -->
	<!-- level 1: patent life -->
	<xs:element name="fr-patent-life">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-jurisprudence" minOccurs="0"/>
				<xs:element ref="fr-inscription" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references-cited" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fr-amended-claim" minOccurs="0"/>
				<xs:element ref="fr-date-availability" minOccurs="0"/>
				<xs:element ref="fr-codetransfo" minOccurs="0"/>
				<xs:element ref="fr-status-list" minOccurs="0"/>
				<xs:element ref="fr-erratas" minOccurs="0"/>
				<xs:element ref="fr-epo-data" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 2: patent life => jurisprudence -->
	<xs:element name="fr-jurisprudence">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 2: patent life => enregistrement RNB -->
	<xs:element name="fr-inscription">
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="registered-number"/>
						<xs:element ref="date"/>
						<xs:element ref="fr-code-inscription" minOccurs="0"/>
						<xs:element ref="fr-nature-inscription"/>
						<xs:element ref="fr-bopinum" minOccurs="0"/>
					</xs:sequence>
					<xs:element ref="text"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3: RNB => inscription code -->
	<xs:element name="fr-code-inscription" type="xs:string"/>
	<!-- level 2: patent life => references-cited -->
	<!-- This tag and elements to be used primarily to code data on the title page of patents under INID code (56)., eg. US patents. For 
		the search report as a sub-document use search-report, eg. for ISR -->
	<xs:element name="references-cited">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence>
					<xs:element ref="citation" maxOccurs="unbounded"/>
					<xs:element ref="place-of-search" minOccurs="0"/>
					<xs:element ref="search-report-publication" minOccurs="0"/>
					<xs:element ref="searcher" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- level 3: references-cited => citations -->
	<xs:element name="citation">
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="patcit"/>
					<xs:element ref="nplcit"/>
				</xs:choice>
				<xs:element ref="corresponding-docs" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="rel-passage" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="category" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="rel-claims" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="citation_attlist"/>
		</xs:complexType>
	</xs:element>
	<!-- Origin of the citation is indicated in attribute "srep-phase" SEA Originates from the Search report APP Cited by the Applicant 
		EXA Revealed during the Examination phase OPP Revealed during the Opposition phase 115 Article 115 (Observation by third parties) OTH 
		Other documents of interest (cited in compte rendu but not in search report) CH2 cited during PCT chapter II (preliminary examination) 
		Patent citations are exchanged in "patcit", as a patent document-id NPL citations are exchanged in "nplcit", as a string of freetext 
		with embedded reference to patent document or NPL Patent document-ids and NPL reference numbers embedded in the NPL citation are extracted 
		and exchanged in "corresponding-docs" Categories are exchanged in "category" in a concatenated string, one occurrence of "category" per 
		citation -->
	<xs:attributeGroup name="citation_attlist">
		<xs:attribute name="srep-phase"/>
	</xs:attributeGroup>
	<!-- level 4 -->
	<!-- Relevant passage group within patent; most used in search reports - use text in abstract, description and claims -->
	<xs:element name="rel-passage">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence maxOccurs="unbounded">
					<xs:element ref="passage" maxOccurs="unbounded"/>
					<xs:element ref="category" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="rel-claims" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- Claims to which cited document is relevant. ST.32 equivalent = B564 -->
	<xs:element name="rel-claims" type="xs:string"/>
	<!-- level 2 : bibliographic-data => claim -->
	<xs:element name="fr-amended-claim" type="xs:string"/>
	<!-- level 2: patent transformation code => codetransfo -->
	<xs:element name="fr-codetransfo">
		<xs:complexType mixed="true">
			<xs:attributeGroup ref="fr-codetransfo_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-codetransfo_attlist">
		<xs:attribute name="date"/>
	</xs:attributeGroup>
	<!-- level 2: patent historic -->
	<xs:element name="fr-status-list">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-status" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3: status of patent historic -->
	<xs:element name="fr-status">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-nature"/>
				<xs:element ref="date" minOccurs="0"/>
				<xs:element ref="p" maxOccurs="unbounded" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-status_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-status_attlist">
		<xs:attribute name="lang"/>
	</xs:attributeGroup>
	<!-- level 2: List of patent erratas -->
	<xs:element name="fr-erratas">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-errata" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 3: Errata -->
	<xs:element name="fr-errata">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-publication-reference" minOccurs="0"/>
				<xs:element ref="text"/>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 2 : European patent data (EP) -->
	<xs:element name="fr-epo-data">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="fr-date-maintain-modified" minOccurs="0"/>
				<xs:element ref="fr-date-translated" maxOccurs="unbounded" minOccurs="0"/>
				<xs:element ref="fr-date-non-translated" maxOccurs="unbounded" minOccurs="0"/>
				<!--[V1.5.2] ref fr-date-claim-translated -->
				<xs:element ref="fr-date-claim-translated" minOccurs="0"/>
				<!--[/V1.5.2] -->
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- level 3 : Content of european patent data -->
	<!-- Date of maintain modified -->
	<xs:element name="fr-date-maintain-modified">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Date of translated -->
	<xs:element name="fr-date-translated">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-date-translated_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-date-translated_attlist">
		<xs:attribute name="revision">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Claims"/>
					<xs:enumeration value="B1"/>
					<xs:enumeration value="B2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- Date of non translated -->
	<xs:element name="fr-date-non-translated">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-date-non-translated_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-date-non-translated_attlist">
		<xs:attribute name="revision">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="B1"/>
					<xs:enumeration value="B2"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>

	<!--[V1.5.2] def fr-date-claim-translated -->
	<xs:element name="fr-date-claim-translated">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--[/V1.5.2] -->


	<!-- End of patent life -->
	<!-- level 1 : CCP data -->
	<xs:element name="fr-ccp-data">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fr-ccp-authorization" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fr-ccp-name"/>
				<xs:element ref="fr-amended-claim" minOccurs="0"/>
				<xs:element ref="fr-inscription" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fr-date-availability" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- level 2 : CCP => Authorization market place -->
	<xs:element name="fr-ccp-authorization">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text"/>
				<xs:element ref="date"/>
			</xs:sequence>
			<xs:attributeGroup ref="fr-ccp-authorization_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="fr-ccp-authorization_attlist">
		<!--[V1.5.3] Store country of market authorization -->
		<xs:attribute name="appm-type" type="xs:string"/>
		<!--[V1.5.3] -->
	</xs:attributeGroup>
	<!-- level 2 : CCP => Name of the drug or phytopharmacological -->
	<xs:element name="fr-ccp-name" type="xs:string"/>
	<!-- End of ccp-data -->
	<!-- Commons elements -->
	<!-- Abstract number -->
	<xs:element name="absno" type="xs:string"/>
	<!-- date article was accepted -->
	<xs:element name="accepted">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- ADDRESSBOOK GROUP: Holds name and contact information for individuals/organizations The LANG attribute is REQUIRED for certain 
		document types, e.g. the JPO PCT Request where names and addresses may be repeated in two different languages. If no structure possible 
		use text. -->
	<xs:element name="addressbook">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:group ref="name_group"/>
					<xs:element ref="address"/>
					<xs:element ref="phone" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="fax" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="email" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="url" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="ead" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="dtext" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="text"/>
			</xs:choice>
			<xs:attributeGroup ref="addressbook_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="addressbook_attlist">
		<xs:attribute name="lang" use="required"/>
	</xs:attributeGroup>
	<!-- address components: Address formats differ from country to country. If no structure possible use text -->
	<xs:element name="address" type="address_group"/>
	<!-- Address, first line -->
	<xs:element name="address-1" type="xs:string"/>
	<!-- Address, second line -->
	<xs:element name="address-2" type="xs:string"/>
	<!-- Address, third line -->
	<xs:element name="address-3" type="xs:string"/>
	<!-- address floor, e.g., 10th -->
	<xs:element name="address-floor" type="xs:string"/>
	<!-- Alternate title for serial eg. BMJ = British Medical Journal -->
	<xs:element name="alttitle" type="xs:string"/>
	<!-- article citations within a 'host' document After patent citations the most common citations are articles within serials (periodicals, 
		magazines), books and electronic (eg. internet) sources. This is a very complex and varied area to standardise. The tag set below is 
		drawn from a number of sources as is the order of the tags (which in any case can be changed depending on the stylesheet used).; The 
		order of tags is that of WIPO ST.14 / ISO 690 layout DROP, J.G. Integrated Circuit Personalization at the Module Level. IBM Tech. Dis. 
		Bull. October 1974, Vol. 17, No. 5, p. 1344 and 1345. [XPnnnnnnnnnnn]. DROP, J.G.Integrated Circuit Personalization at the Module LevelIBM 
		Tech. Dis. Bull.197410001751344 and 1345XP1234456 Note: it is NOT expected that the applicant will create the tagged data - it can be 
		generated from a 'pull down' data entry form (eg. in Word) or generated from internal SR databases -->
	<xs:element name="article">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence>
					<xs:element ref="author" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="atl" minOccurs="0"/>
					<xs:element ref="subname" minOccurs="0" maxOccurs="unbounded"/>
					<xs:choice>
						<xs:element ref="serial"/>
						<xs:element ref="book"/>
					</xs:choice>
					<xs:element ref="absno" minOccurs="0"/>
					<xs:element ref="location" minOccurs="0"/>
					<xs:element ref="class" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="keyword" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cpyrt" minOccurs="0"/>
					<xs:element ref="artid" minOccurs="0"/>
					<xs:element ref="refno" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- Article unique ID - assigned by a publisher to an article -->
	<xs:element name="artid" type="xs:string"/>
	<!-- article title -->
	<xs:element name="atl" type="xs:string"/>
	<!-- applicant's legal representative -->
	<xs:element name="attorney-or-agent">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook"/>
				<xs:element ref="range-of-authority" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attributeGroup ref="attorney-or-agent_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="attorney-or-agent_attlist">
		<xs:attribute name="rep-type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="agent"/>
					<xs:enumeration value="attorney"/>
					<xs:enumeration value="common-representative"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<!-- author Prime responsibility for the work there is no distinguishing between personal and corporate authors author element followed 
		by name or addressbook information use the ID attribute if, for example, there is a need to distinguish between multiple authors. Can 
		also include affiliation details Recommended id = auth01, auth02, etc. -->
	<xs:element name="author">
		<xs:complexType>
			<xs:choice>
				<xs:group ref="name_group"/>
				<xs:element ref="addressbook"/>
			</xs:choice>
			<xs:attributeGroup ref="author_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="author_attlist">
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- Availability and access - REQUIRED use for URL, online database, ftp address, email, etc.; e.g. Available from Internet: http://www.epo.org 
		Available from DIALOG Information Systems, Palo Alto (Calif.) -->
	<xs:element name="avail" type="xs:string"/>
	<!-- bold formatting for text -->
	<xs:element name="b">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="smallcaps"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- biological accession number: the identification assigned to the biological material by the depository -->
	<xs:element name="bio-accno" type="xs:string"/>
	<!-- biological deposit citation (US EFS: biological-deposit) According to PCT Rule 13bis, EPC Rule 28 This covers all bio data references 
		long and short within the description and/or claims Recommended id = biod01, biod02, etc. -->
	<xs:element name="bio-deposit">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="depositary"/>
				<xs:element ref="bio-accno"/>
				<xs:element ref="date" minOccurs="0"/>
				<xs:element ref="term" minOccurs="0"/>
				<xs:element ref="dtext" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="bio-deposit_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="bio-deposit_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="num"/>
		<xs:attribute name="url"/>
		<xs:attribute name="dnum"/>
	</xs:attributeGroup>
	<!-- book citation In the order specified by WIPO ST.14 and ISO 690 and AACR2 rules for layout WALTON, Herrmann. Microwave quantum theory. 
		London : Sweet and Maxwell, 1973. ISBN 0123456789. Vol.2, p. 138 to 192. WALTON, HerrmanMicrowave quantum theoryLondonSweet and Maxwell19730000138 
		to 1920-1234-568-9 -->
	<xs:element name="book">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence>
					<xs:element ref="author" minOccurs="0" maxOccurs="unbounded"/>
					<xs:choice>
						<xs:element ref="book-title" maxOccurs="unbounded"/>
						<xs:element ref="conference"/>
					</xs:choice>
					<xs:sequence>
						<xs:element ref="subtitle" minOccurs="0"/>
						<xs:element ref="subname" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="edition" minOccurs="0"/>
						<xs:element ref="imprint" minOccurs="0"/>
						<xs:element ref="descrip" minOccurs="0"/>
						<xs:element ref="series" minOccurs="0"/>
						<xs:element ref="absno" minOccurs="0"/>
						<xs:element ref="location" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="isbn" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="pubid" minOccurs="0"/>
						<xs:element ref="vid" minOccurs="0"/>
						<xs:element ref="bookno" minOccurs="0"/>
						<xs:element ref="notes" minOccurs="0"/>
						<xs:element ref="class" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="keyword" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="cpyrt" minOccurs="0"/>
						<xs:element ref="refno" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- br - break, line break within a paragraph -->
	<!-- book number -->
	<xs:element name="bookno" type="xs:string"/>
	<!-- the title of the cited book -->
	<xs:element name="book-title" type="xs:string"/>
	<xs:element name="br">
		<xs:complexType/>
	</xs:element>
	<!-- building information in address, e.g., Hoffman building -->
	<xs:element name="building" type="xs:string"/>
	<xs:element name="category" type="xs:string"/>
	<!-- chapter, e.g., in a book. Example: Chapter 2 -->
	<xs:element name="chapter" type="xs:string"/>
	<xs:element name="chem">
		<xs:complexType>
			<xs:attributeGroup ref="chem_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="chem_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="file"/>
		<xs:attribute name="chem-type" use="required"/>
	</xs:attributeGroup>
	<!-- chemistry Chemical formulae, ring structures, Markush structures, etc. For now treat as image data. Depending on industry support 
		we may use, e.g., CML later Recommended id = chem01, chem02, etc -->
	<xs:element name="chemistry">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="img"/>
				<xs:sequence>
					<xs:element ref="chem"/>
					<xs:element ref="img" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
			<xs:attributeGroup ref="chemistry_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="chemistry_attlist">
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="num"/>
	</xs:attributeGroup>
	<xs:element name="chf">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="br"/>
				<xs:element ref="fdist"/>
				<xs:element ref="chfbr"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="chfbr">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="br"/>
				<xs:element ref="fdist"/>
			</xs:choice>
			<xs:attributeGroup ref="chfbr_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="chfbr_attlist">
		<xs:attribute name="type" default="none">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="bar"/>
					<xs:enumeration value="none"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="city" type="xs:string"/>
	<!-- In NPL, use this element for a classification code; e.g. ipc, dewey etc. In IPCR, use this for the IPC class symbol. -->
	<xs:element name="class" type="xs:string"/>
	<!-- cited document column, first -->
	<xs:element name="colf" type="xs:string"/>
	<!-- cited document column, last -->
	<xs:element name="coll" type="xs:string"/>
	<!-- cited document column numbers -->
	<xs:element name="column">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="colf"/>
				<xs:element ref="coll"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- CONFERENCE GROUP / CITATION There is no standard way of citing conference proceedings; date of the conf. may come before or after 
		place, for example. The tags are specified in a set order where it is not possible or correct then simply use text -->
	<xs:element name="conference">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="text"/>
				<xs:sequence>
					<xs:element ref="conftitle"/>
					<xs:element ref="date" minOccurs="0"/>
					<xs:element ref="confno" minOccurs="0"/>
					<xs:element ref="confplace" minOccurs="0"/>
					<xs:element ref="confsponsor" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- Conference number -->
	<xs:element name="confno" type="xs:string"/>
	<!-- Conference place -->
	<xs:element name="confplace">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="address" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Conference sponsor -->
	<xs:element name="confsponsor">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="addressbook" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Conference title -->
	<xs:element name="conftitle" type="xs:string"/>
	<!-- & docs. : Patent family and corresponding-docs -->
	<xs:element name="corresponding-docs">
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="document-id"/>
				<xs:element ref="nplcit"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- Country: use ST.3 country code, e.g. DE, FR, GB, NL, etc. Also includes EP, WO -->
	<xs:element name="country" type="xs:string"/>
	<!-- County, parish, department, etc (mainly UK, USA) -->
	<xs:element name="county" type="xs:string"/>
	<!-- copyright info. Copyright of publisher of serial -->
	<xs:element name="cpyrt" type="xs:string"/>
	<!-- Cross reference to any other element with an identifier (id) -->
	<xs:element name="crossref">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
			</xs:choice>
			<xs:attributeGroup ref="crossref_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="crossref_attlist">
		<xs:attribute name="idref" type="xs:IDREF" use="required"/>
	</xs:attributeGroup>
	<!-- Recommended format: YYYYMMDD -->
	<xs:element name="date" type="xs:string"/>
	<!-- date cited - date on which the citation was cited. May be different from srchdate below -->
	<xs:element name="datecit">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- definition description -->
	<xs:element name="dd">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
				<xs:element ref="br"/>
				<xs:element ref="pre"/>
				<xs:element ref="patcit"/>
				<xs:element ref="nplcit"/>
				<xs:element ref="bio-deposit"/>
				<xs:element ref="crossref"/>
				<xs:element ref="figref"/>
				<xs:element ref="img"/>
				<xs:element ref="ul"/>
				<xs:element ref="ol"/>
				<xs:element ref="chemistry"/>
				<xs:element ref="maths"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="deceased-inventor">
		<xs:complexType>
			<xs:group ref="name_group"/>
			<xs:attributeGroup ref="deceased-inventor_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="deceased-inventor_attlist">
		<xs:attribute name="sequence"/>
		<xs:attribute name="lang"/>
	</xs:attributeGroup>
	<!-- department, division, section, etc. name -->
	<xs:element name="department" type="xs:string"/>
	<!-- information regarding the facility storing the biological deposit -->
	<xs:element name="depositary">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="address" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Physical description - generally NOT used in citations in pats. eg. v., 27cm -->
	<xs:element name="descrip" type="xs:string"/>
	<xs:element name="designated-states">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="region" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="designated-states-as-inventor">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="region" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- definition list Recommended id = dlis01, dlis02, etc. -->
	<xs:element name="dl">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="dt"/>
				<xs:element ref="dd"/>
			</xs:sequence>
			<xs:attributeGroup ref="dl_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="dl_attlist">
		<xs:attribute name="tsize"/>
		<xs:attribute name="compact">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="compact"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- the number of the referenced patent (or application) document -->
	<xs:element name="doc-number" type="xs:string"/>
	<!-- Document identification refers to patents (and patent applications) only. See WIPO ST.14 -->
	<xs:element name="document-id">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element ref="doc-number"/>
				<xs:element ref="kind" minOccurs="0"/>
				<xs:element ref="name" minOccurs="0"/>
				<xs:element ref="date" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="document-id_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="document-id_attlist">
		<xs:attribute name="lang"/>
	</xs:attributeGroup>
	<!-- term to be defined; Recommended id = dtrm01, dtrm02, etc. -->
	<xs:element name="dt">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
			</xs:choice>
			<xs:attributeGroup ref="dt_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="dt_attlist">
		<xs:attribute name="id" type="xs:ID"/>
	</xs:attributeGroup>
	<!-- Descriptive text -->
	<xs:element name="dtext" type="xs:string"/>
	<!-- Electronic addresses other than email -->
	<xs:element name="ead" type="xs:string"/>
	<!-- end date used in pubdate -->
	<xs:element name="edate" type="xs:string"/>
	<!-- edition statement -->
	<xs:element name="edition" type="xs:string"/>
	<xs:element name="effective-in">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- email address -->
	<xs:element name="email" type="xs:string"/>
	<xs:element name="emr">
		<xs:complexType>
			<xs:attributeGroup ref="emr_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="emr_attlist">
		<xs:attribute name="source" type="xs:IDREFS"/>
	</xs:attributeGroup>
	<!-- a TIFF image of a signature -->
	<xs:element name="fax" type="xs:string"/>
	<xs:element name="fdist">
		<xs:complexType/>
	</xs:element>
	<!-- a reference to a figure; e.g., "...as shown in figure 1..." -->
	<xs:element name="figref">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="b"/>
				<xs:element ref="i"/>
				<xs:element ref="u"/>
				<xs:element ref="sup"/>
				<xs:element ref="sub"/>
				<xs:element ref="smallcaps"/>
			</xs:choice>
			<xs:attributeGroup ref="figref_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="figref_attlist">
		<xs:attribute name="idref" type="xs:IDREFS"/>
		<xs:attribute name="num"/>
	</xs:attributeGroup>
	<!-- First or given name(s), initial(s) -->
	<xs:element name="first-name" type="xs:string"/>
	<xs:element name="fla">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="flac" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="flac">
		<xs:complexType mixed="true">
			<xs:attributeGroup ref="flac_attlist"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="flac_attlist">
		<xs:attribute name="pos" default="ABOVE">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="ABOVE"/>
					<xs:enumeration value="MID"/>
					<xs:enumeration value="BELOW"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="fr-bopinum" type="xs:string"/>
	<!-- Date of application refused -->
	<xs:element name="fr-date-application-refused">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text" minOccurs="0"/>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Date of application withdrawn -->
	<xs:element name="fr-date-application-withdrawn">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="text" minOccurs="0"/>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Date of availabilities (refused, withdrawn, lapsed, granted, search added, search report mailed, limit validity, last fee payement, 
		search supplemental, ...) -->
	<xs:element name="fr-date-availability">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fr-date-application-refused"/>
				<xs:element ref="fr-date-application-withdrawn"/>
				<xs:element ref="fr-date-lapsed"/>
				<!--[V1.5.2] ref fr-date-notification-lapsed -->
				<xs:element ref="fr-date-notification-lapsed"/>
				<!--[/V1.5.2] -->
				<xs:element ref="fr-date-granted"/>
				<xs:element ref="fr-date-search-completed"/>
				<xs:element ref="fr-date-search-added"/>
				<xs:element ref="fr-date-search-report-mailed"/>
				<!--[V1.5.3] ref fr-date-claim-modified <xs:element ref="fr-date-claim-modified"/> [/V1.5.3] -->
				<xs:element ref="fr-date-limit-validity"/>
				<xs:element ref="fr-last-fee-payement"/>
				<!-- [V1.5.2] ref fr-next-fee-payement -->
				<xs:element ref="fr-next-fee-payement"/>
				<!-- [/V1.5.2] -->
				<xs:element ref="fr-date-search-supplemental"/>
				<xs:element ref="fr-date-prorogation-published"/>
				<xs:element ref="fr-date-prorogation-applied"/>
				<!--[V1.5.3] -->
				<xs:element ref="fr-date-prorogation-granted"/>
				<!-- [/V1.5.3] -->
				<xs:element ref="fr-date-limit-prorogation"/>
				<xs:element ref="fr-date-prorogation-refused"/>
