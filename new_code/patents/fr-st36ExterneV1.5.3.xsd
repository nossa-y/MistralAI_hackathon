		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Date of prorogation refused -->
	<xs:element name="fr-date-prorogation-refused">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
				<xs:element ref="fr-bopinum" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="fr-docref">
		<xs:complexType mixed="true">
			<xs:attributeGroup ref="fr-docref_attlist"/>
